{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ],
    "unified-signatures": false,
    "no-empty-interface": {
      "severity": "warning"
    },
    "ban-types": false,
    "no-string-literal": false
  }
}
